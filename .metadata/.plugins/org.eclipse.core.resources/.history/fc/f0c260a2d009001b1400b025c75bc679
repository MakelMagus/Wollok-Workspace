object aldea {
	var property size = 0
	
	var property people = [] 
	var property terrains = []
	
	method workable() {return true}
	method work(amount) {
		size += amount/10
	}
	
}

object food {
	var property amount = 0
	
	method workable() {return false}
	method consume(food_consumed){
		amount -= food_consumed
	}
}